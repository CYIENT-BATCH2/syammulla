/*
x=30, y=20, z=10 FIND OUT THE VALUE FOR K

1)k = x + y * z / 4 % 2 - 1 ;

2)k= x% y+z*z;

3)k=  x*x-y/z;

4)k= x+y/z-y; 

5)y=x+y-(x=y); //y=?

6)y=x+y-x=y    //y=?*/
#include<stdio.h>
int main()
{
    int x=30,y=20,z=10,k;
    k=x+y*z/4%2-1;
    printf("k=%d\n",k);
    k= x% y+z*z;
    printf("k=%d\n",k);
    k=  x*x-y/z;
    printf("k=%d\n",k);
    k= x+y/z-y;
    printf("k=%d\n",k);
    y=x+y-(x=y); //y=?
    printf("y=%d\n",y);
    y=x+y-(x=y);   //y=?*/
    printf("y=%d\n",y);
    
}


